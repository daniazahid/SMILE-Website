						<label for="physicianPhone"><h4>Physician's Phone:</h4></label>
						<input type="text" class="form-control"  name="physicianPhone" id="physicianPhone" pattern="[0-9]({11}|{10})" title="Phone number must have correct number of digits">
			
						<label for="lastvisit"><h4>Date of Last Visit/Exam:</h4></label>
						<input type="date" class="form-control"  name="physicianLastvisit" id="lastvisit">
					</div>
					<br>
				</div>
				
				<div class="form-row">
